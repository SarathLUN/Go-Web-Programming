    FIRST NAME: {{.FirstName}}
    <br>
    LAST NAME: {{.LastName}}
{{else}}
    <h2><a href="/login">Login</a></h2>
    <h2><a href="/signup">Sign Up</a></h2>

{{end}}
<h2>Go to the <a href="/bar">BAR</a></h2>
</body>
</html>